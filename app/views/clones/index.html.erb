<p id="notice"><%= notice %></p>

<h1>Listing Clones</h1>

<table>
  <thead>
    <tr>
      <th>Person</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clones.each do |clone| %>
      <tr>
        <td><%= clone.person %></td>
        <td><%= link_to 'Show', clone %></td>
        <td><%= link_to 'Edit', edit_clone_path(clone) %></td>
        <td><%= link_to 'Destroy', clone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clone', new_clone_path %>
